programa {
  funcao inicio() {
  
  real m, km, dc, cm, mm

  escreva("Digite a quantidade de metros: ")
  leia(m)

  km = m/1000

  dc = m*10

  cm = m*100
  
  mm = m*1000

  escreva(" A quantidade de km é: ", km)

  escreva(" A quantidade de dc é: ", dc)

  escreva(" A quantidade de cm é: ", cm)

  escreva(" A quantidade de mm é: ", mm)

    
  }
}
